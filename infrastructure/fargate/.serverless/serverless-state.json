                          "s3:PutObjectAcl",
                          "s3:GetObject",
                          "s3:GetObjectAcl",
                          "s3:ListBucket"
                        ],
                        "Resource": {
                          "Fn::Join": [
                            "",
                            [
                              "arn:aws:s3:::",
                              {
                                "Ref": "TerriamapSharingBucket"
                              },
                              "/*"
                            ]
                          ]
                        }
                      }
                    ]
                  }
                }
              ]
            }
          },
