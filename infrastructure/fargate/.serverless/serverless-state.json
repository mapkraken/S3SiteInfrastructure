                      "Value": "5432"
                    },
                    {
                      "Name": "PG_DATABASE",
                      "Value": "koop"
                    },
                    {
                      "Name": "PG_OBJECTID",
                      "Value": "gid"
                    },
                    {
                      "Name": "PG_LIMIT",
                      "Value": "1000"
                    },
                    {
                      "Name": "PG_USER",
                      "Value": "{{resolve:secretsmanager:sit/rds/postgis-secret:SecretString:username}}"
                    },
                    {
                      "Name": "PG_PASSWORD",
                      "Value": "{{resolve:secretsmanager:sit/rds/postgis-secret:SecretString:password}}"
                    },
                    {
                      "Name": "PG_SSL",
                      "Value": "true"
                    }
                  ],
